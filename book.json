{
    "title": "Linkkey Documentation",
    "description": "Reusable functions and components for the SNS apps ",
    "plugins": [
        "-sharing",
        "livereload",
        "hide-element",
        "search-pro",
        "expandable-chapters",
        "chapter-fold",
        "sectionx",
        "splitter",
        "lightbox",
        "copy-code-button",
        "back-to-top-button",
        "anchor-navigation-ex",
        "favicon",
        "insert-logo",
        "sharing-plus",
        "github",
        "theme-comscore"
    ],
    "variables": {
        "links": {}
    },
    "pluginsConfig": {
        "hide-element": {
            "elements": [
                ".gitbook-link"
            ]
        },
        "anchor-navigation-ex": {
            "multipleH3": false,
            "showLevel": false
        },
        "favicon": {
            "shortcut": "assets/img/favicon.ico"
        },
        "insert-logo": {
            "url": "/assets/img/linkkey_logo.png",
            "style": "max-height: 60px; min-height: 60px; border-radius:16px;box-shadow: 0 6px 12px rgb(0 0 0 / 18%);"
        },
        "github": {
            "url": "https://github.com/Link-Key"
        },
        "sharing-plus": {
            "twitter": true
        }
    }
}