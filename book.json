{
    "title": "Linkkey Documentation",
    "description": "Reusable functions and components for the SNS apps ",
    "plugins": [
        "hide-element",
        "search-pro",
        "expandable-chapters",
        "chapter-fold",
        "sectionx",
        "splitter",
        "lightbox",
        "copy-code-button",
        "back-to-top-button",
        "pageview-count",
        "page-toc-button"
    ],
    "pluginsConfig": {
        "hide-element": {
            "elements": [
                ".gitbook-link"
            ]
        },
        "page-toc-button": {
            "maxTocDepth": 2,
            "minTocSize": 2
        }
    }
}